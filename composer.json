{
    "name": "cherish/ly",
    "description": "Cherish admin is a component of laravel, vue, element build backend system.",
    "keywords": ["laravel", "admin", "vue", "element", "laravel admin", "vue admin", "element admin"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "cherish",
            "email": "cherish@qq.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "spatie/laravel-permission": "~3.2",
        "laravel/sanctum": "^2.4"
    },
    "require-dev": {
        "orchestra/testbench": "~5.1.0",
        "mockery/mockery": "^1.0",
        "orchestra/database": "~5.1.0",
        "doctrine/dbal": "^2.8",
        "dms/phpunit-arraysubset-asserts": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cherish\\Ly\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cherish\\Ly\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cherish\\Ly\\Providers\\LyServiceProvider"
            ]
        }
    },
    "minimum-stability":"dev",
    "prefer-stable": true
}
